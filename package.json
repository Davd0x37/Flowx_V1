{
  "name": "flowx",
  "packageManager": "pnpm@8.6.6",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --config prettier.config.cjs --write \"**/*.{ts,tsx,html,scss,json,yml,md,vue}\"",
    "api:dev": "pnpm run --filter api dev",
    "api:dev:ts-node": "pnpm run --filter api dev:ts-node",
    "web:dev": "pnpm run --filter web dev",
    "web:test": "pnpm run --filter web test"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-vue": "^9.15.1",
    "eslint": "^8.44.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prettier": "^2.8.7",
    "typescript": "^5.1.6",
    "vue-eslint-parser": "^9.3.1"
  }
}
